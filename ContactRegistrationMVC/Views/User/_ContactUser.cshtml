@model List<ContactModel>

<table class="table" id="table-contacts-user">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">E-mail</th>
            <th scope="col">Phone number</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Any())
        {
            foreach (var contact in Model)
            {
                <tr>
                    <th scope="row">@contact.Id</th>
                    <td>@contact.Name</td>
                    <td>@contact.Email</td>
                    <td>@contact.Number</td>
                </tr>
            }
        }
        else
        {
            <tr class="odd"><td valign="top" colspan="5">Does not have contacts</td></tr>
        }
    </tbody>
</table>
